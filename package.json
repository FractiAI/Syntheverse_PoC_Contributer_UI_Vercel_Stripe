    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-icons": "^3.0.0",
    "chai": "^6.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "hardhat-deploy": "^1.0.4",
