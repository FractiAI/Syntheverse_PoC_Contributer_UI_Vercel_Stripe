    "@types/three": "^0.182.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "drizzle-orm": "^0.33.0",
    "ethers": "^6.16.0",
    "lucide-react": "^0.428.0",
    "next": "14.2.35",
    "postgres": "^3.4.4",
    "react": "^18",
    "react-dom": "^18",
