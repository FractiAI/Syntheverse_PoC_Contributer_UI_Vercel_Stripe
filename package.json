{
  "name": "syntheverse-operator-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "stripe:setup": "tsx --env-file=.env stripeSetup.ts",
    "stripe:listen": "stripe listen --forward-to localhost:3000/webhook/stripe",
    "test": "mocha --require tsx/cjs --require ./tests/setup-env.cjs --timeout 300000 'tests/**/*.test.ts'",
    "test:hardhat": "mocha --require tsx/cjs --require ./tests/setup-env.cjs --require tsconfig-paths/register --timeout 300000 'tests/hardhat/**/*.test.ts'",
    "test:integration": "mocha --require tsx/cjs --require ./tests/setup-env.cjs --timeout 300000 'tests/integration/**/*.test.ts'",
    "test:security": "mocha --require tsx/cjs --require ./tests/setup-env.cjs --timeout 300000 'tests/security/**/*.test.ts'",
    "test:load": "mocha --require tsx/cjs --require ./tests/setup-env.cjs --timeout 600000 'tests/load/**/*.test.ts'",
    "test:all": "tsx tests/run-all.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
